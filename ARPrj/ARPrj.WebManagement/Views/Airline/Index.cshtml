@model IEnumerable<ARPrj.Model.Models.AirLine.AirlineModel>

@{
    ViewBag.Title = "Index";
}
<div class="content-header">

    <div class="leftside-content-header">

        <ul class="breadcrumbs">
            <li><a href="/">Trang chủ</a></li>
            <li><a href="/">Airline</a></li>
        </ul>
    </div>
</div>
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AirlineId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AirlineName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NetPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdateDate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AirlineId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AirlineName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NetPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdateDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.AirlineId }) |
                @Html.ActionLink("Details", "Details", new { id = item.AirlineId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.AirlineId })
            </td>
        </tr>
    }

</table>
